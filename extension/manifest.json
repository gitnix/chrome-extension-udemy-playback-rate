{
	"name": "Udemy Video Playback Speed",
	"version": "1.2.0",
	"description": "Adds additional playback speed options for videos on Udemy.",
	"manifest_version": 2,
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"permissions": ["storage"],
	"content_scripts": [
		{
			"matches": ["https://www.udemy.com/*"],
			"js": ["playbackRate.js"],
			"css": ["menuList.css"]
		}
	]
}
